page{
  background-color: #eeeeee;
  color: #333;
}
/* 按钮 */
.button-primary,.button-info,.button-danger{
  border-radius: 10rpx;
  font-size: 32rpx;
  line-height: 80rpx;
  text-align: center;
}
.button-primary{
  background-color: #1296db;
  color: #fff;
}
.button-info{
  background-color: #ffffff;
  box-shadow: 0 0 5rpx 3rpx #dedede;
  color: #333;
}
.button-danger{
  background-color: #d81e06;
  color: #fff;
}
.button-primary:hover{
  opacity: 0.9;
}
.button-info:hover{
  opacity: 0.9;
}
.button-danger:hover{
  opacity: 0.9;
}
/* 无内容 */
.hint-card{
    margin-bottom: 10rpx;
    background-color: #fff;
    border-radius: 10rpx;
    box-sizing: border-box;
    height: 150rpx;
    position: relative;
    line-height: 150rpx;
    text-align: center;
    color: #666;
    font-size: 30rpx;
}
/* row行 */
.rows{
  position: relative;
  height: 120rpx;
  border-bottom: 2rpx solid #eee;
  margin-bottom: 10rpx;
}
.rows::before{
  border:none;
}
.rows-title{
  font-size: 28rpx;
}
.rows50{
  position: relative;
  display: inline-block;
  width: 50%;
  height: 120rpx;
  margin-bottom: 10rpx;
}
.rows-val,.rows-val50{
  border-radius: 10rpx;
  color: #333;
  line-height: 80rpx;
  box-sizing: border-box;
  height: 80rpx;
  border: 1px solid transparent;
}
.rows-val50{
  border-bottom: 2rpx solid #eee;
  border-radius: 0;
}
.rows-more{
  width: 30rpx;
  height: 30rpx;
  position:absolute;
  bottom:25rpx;
  right: 20rpx;
}
/* 地址卡 */
.site-item{
  margin-bottom: 10rpx;
  background-color: #fff;
  border-radius: 10rpx;
  box-sizing: border-box;
  height: 150rpx;
  position: relative;
}
.site-none{
  height: 120rpx;
  line-height: 120rpx;
}
.site-title,.site-address{
  height: 40rpx;
  width: 600rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  word-break: break-all;
}
.site-title{
  font-size: 26rpx;
}
.site-address{
  font-size: 32rpx;
  font-weight: bold;
}
.site-content{
  height: 40rpx;
}
.site-name,.site-phone{
  padding-right: 20rpx;
  font-size: 26rpx;
}
.site-more{
  width: 30rpx;
  height: 30rpx;
  position:absolute;
  top:46rpx;
  right: 20rpx;
}
.site-edit{
  position:absolute;
  right: 20rpx;
  top:55rpx;
  width: 50rpx;
  height: 50rpx;
}
.site-edit image{
  width: 50rpx;
  height: 50rpx;
}
.site-default{
  padding:2rpx 8rpx;
  margin-right: 10rpx;
  font-size: 20rpx;
  background-color: #d81e06;
  border-radius: 8rpx;
  color: #fff;
}
/* 商品 */

.card-item{
  background-color: #fff;
  border-bottom: 2rpx solid #eee;
  padding: 20rpx 20rpx 20rpx 0;
  position: relative;
}
.card-img{
  background-color: #eee;
  width: 150rpx;
  height: 150rpx;
  border-radius: 5rpx;
  position: absolute;
  top: 20rpx;
  left:0;
}
.card-content{
  margin-left:170rpx;
  height:150rpx;
}
.card-name,.card-details{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  word-break: break-all;
  -webkit-line-clamp: 1;
}
.card-name{
  font-weight: bold;
  font-size: 32rpx;
  height: 50rpx;
  line-height: 50rpx;
}
.card-details{
  font-size: 28rpx;
  color: #666;
  height: 30rpx;
  line-height: 30rpx;
}
.card-price{
  padding-top: 30rpx;
  font-size:24rpx;
  line-height: 40rpx;
  color: #d81e06;
  height: 40rpx;
}
.card-count{
  position: absolute;
  right: 0;
  bottom: 20rpx;
  font-size:24rpx;
}

/*  */
.border-dotted{
  border-bottom: 4rpx dotted #eee;
}

/* 骨架 */
/* 地址骨架 */
.ske-site-title,.ske-site-address{
  background-color: #eee;
  border-radius:5rpx;
  margin: 10rpx 0;
}
.ske-site-title{
  height:30rpx;
  width: 300rpx;
}
.ske-site-address{
  height:40rpx;
  width:400rpx;
}
.ske-site-name,.ske-site-phone{
  background-color: #eee;
  height:20rpx;
  border-radius:5rpx;
  display: inline-block;
  vertical-align: top;
}
.ske-site-name{
  width:100rpx;
}
.ske-site-phone{
  width:150rpx;
  margin:0 0 0 10rpx;
}
/* 商品骨架 */
.ske{
  background-color: #eee;
  border-radius: 5rpx;
}
.ske-card{
  height: 100vh;
  overflow: hidden;
}
.ske-card-name{
  height: 45rpx;
  width: 180rpx;
  margin: 0 0 10rpx;
}
.ske-card-details{
  height: 30rpx;
  width: 500rpx;
  margin: 10rpx 0;
}
.ske-card-price{
  height: 28rpx;
  width: 150rpx;
  margin-top: 40rpx;
}
.ske-count{
  height:40rpx;
  width: 120rpx;
  position: absolute;
  bottom: 20rpx;
  right: 20rpx;
}
/* 订单 */
.ske-order-text{
  height: 20rpx;
  margin: 10rpx 0;
  background-color: #eee;
  border-radius: 10rpx;
  width: 300rpx;
}